# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Role Key (for bulk upload script only)
# Get from: Supabase Dashboard → Settings → API → Service Role Key
# ⚠️ Keep this secret! Never commit to git or expose publicly
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Default User ID for bulk uploads (UUID format)
# Get from: Supabase Dashboard → Authentication → Users
DEFAULT_USER_ID=your_user_uuid

# Netlify Functions URL (for AI tagging)
# Production: https://your-app.netlify.app/.netlify/functions
# Local dev: http://localhost:8888/.netlify/functions
VITE_NETLIFY_FUNCTIONS_URL=https://your-app.netlify.app/.netlify/functions

# OpenAI API for voice transcription (Whisper) and photo analysis
VITE_OPENAI_API_KEY=your_openai_api_key

# Claude API for AI parsing and analysis
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key

# AssemblyAI API for sales coach transcription with speaker diarization
VITE_ASSEMBLYAI_API_KEY=your_assemblyai_api_key
